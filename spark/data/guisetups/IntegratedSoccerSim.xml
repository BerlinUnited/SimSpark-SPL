<root>
  <name>IntegratedSoccerSim</name>
  <addplugins reloadall="true">
    <frame caption="Log" reload="false" name="logframe" attachment="bottom_attachment">
      <attachments>
        <attachment>left_attachment</attachment>
      </attachments>
    </frame>
    <frame caption="Soccer Control" name="soccercontrolframe" attachment="right_attachment"/>
    <frame caption="Scenegraph" name="scenegraphframe" attachment="left_attachment"/>
    <frame caption="Simulation" name="simulationframe" attachment="left_attachment"/>
    <frame caption="Monitor" name="monitorframe" attachment="center_attachment">
      <attachments>
        <attachment>right_attachment</attachment>
      </attachments>
    </frame>
  </addplugins>
  <removeplugins removeall="true"/>
  <tasks>
    <serverthread third="spark.rb" second="rcssserver3dintegrated.rb" priority="7" first="" name="Server"/>
    <process third="" second="" priority="7" first="rcssagent3d.exe" name="Agent_1"/>
    <process third="" second="" priority="7" first="rcssagent3d.exe" name="Agent_2"/>
    <process third="" second="" priority="7" first="rcssagent3d.exe" name="Agent_3"/>
    <process third="" second="" priority="7" first="rcssagent3d.exe" name="Agent_4"/>
    <process third="" second="" priority="7" first="rcssagent3d.exe" name="Agent_5"/>
    <process third="" second="" priority="7" first="rcssagent3d.exe" name="Agent_6"/>
  </tasks>
</root>
